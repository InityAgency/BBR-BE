<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Approved Claim Request</title>
    <!--[if !mso]><!-->
    <link href="https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400..900;1,400..900&display=swap" rel="stylesheet">
    <!--<![endif]-->
    <style type="text/css">
        /* Import Playfair Display font for email clients that support it */
        @import url('https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400..900;1,400..900&display=swap');
        
        /* Fallback font stack */
        @font-face {
            font-family: 'Playfair Display';
            font-style: normal;
            font-weight: 400;
            src: local('Playfair Display Regular'), local('PlayfairDisplay-Regular'), url(https://fonts.gstatic.com/s/playfairdisplay/v21/nuFiD-vYSZviVYUb_rj3ij__anPXDTzYgEM86xQ.woff2) format('woff2');
        }
        
        @font-face {
            font-family: 'Playfair Display';
            font-style: normal;
            font-weight: 700;
            src: local('Playfair Display Bold'), local('PlayfairDisplay-Bold'), url(https://fonts.gstatic.com/s/playfairdisplay/v21/nuFlD-vYSZviVYUb_rj3ij__anPXBYf9lW4e5j5hNKc.woff2) format('woff2');
        }
        
        /* Basic reset */
        body, p, h1, h2, h3, h4, h5, h6, table, td, div, a, span {
            margin: 0;
            padding: 0;
        }
        
        body, p, td, div, a, span {
            font-family: Arial, Helvetica, sans-serif;
        }
        
        h1, h2, h3, h4, h5, h6 {
            font-family: 'Playfair Display', serif !important;
            letter-spacing: -0.5px;
            font-weight: 500;
        }
        /* Main styles */
        body {
            background-color: #f4f4f4;
            width: 100%;
            margin: 0;
            padding: 0;
        }
        
        .email-container {
            max-width: 600px;
            margin: 0 auto;
            background-color: #000;
            position: relative;
        }
        
        
        .logo {
            max-width: 180px;
            padding: 20px 0;
            margin-top: 40px;
        }
        .content-wrapper {
            padding: 40px;
            background-color: #000;
        }
        .content {
            padding: 40px 30px;
            text-align: center;
            background-color: #F8F6F0;
            width: 100%;
            max-width: calc(100% - 120px);
            margin: 0 auto;
            margin-left: 30px;
            margin-right: 30px;
            margin-bottom: 30px;
            display: inline-block;
        }
        
        .main-title {
            font-size: 34px;
            color: #000000;
            margin-bottom: 20px;
            line-height: 1.4;
        }
        h2{
            font-size: 24px;
        }
        
        .welcome-text {
            color: #666666;
            font-size: 16px;
            line-height: 1.5;
            margin-bottom: 25px;
        }
        
        .features-container {
            display: inline-block;
            width: 100%;
            margin: 20px 0;
            text-align: center;
        }
        
        .feature-box {
            display: inline-block;
            width: 90%;
            vertical-align: top;
            margin: 0 1.5%;
            padding: 15px 0;
            border: 1px solid rgba(198, 163, 99, 0.50);
            border-radius: 5px;
            background-color: #fff;
            min-height: 160px;
        }
        
        .feature-box svg {
            width: 40px;
            height: 40px;
            margin: 0 auto 10px;
            display: block;
            color: #8b6d40;
            margin-left: 10px;
        }
        
        .feature-title {
            font-size: 14px;
            color: #666666;
            padding: 0 10px;
            line-height: 1.4;
        }
        
        .no-login {
            font-family: 'Playfair Display', Georgia, serif !important;
            margin: 25px 0 15px;
        }
        
        .link-instruction {
            color: #666666;
            font-size: 14px;
            margin-bottom: 15px;
        }
        
        .cta-button {
            display: block;
            background-color: #c9a86b;
            color: #ffffff !important;
            text-decoration: none;
            padding: 15px 20px;
            margin: 20px auto;
            max-width: 300px;
            text-align: center;
            border-radius: 3px;
        }
        
        .divider {
            height: 1px;
            background-color: #e0e0e0;
            margin: 30px 0;
        }
        
        .closing-title {
            font-family: 'Playfair Display', Georgia, serif !important;
            margin-bottom: 20px;
        }
        
        .closing-text {
            color: #666666;
            font-size: 16px;
            line-height: 1.5;
            margin-bottom: 20px;
        }
        
        .signature {
            color: #666666;
            font-size: 16px;
            margin-top: 20px;
        }
        
        .footer {
            background-color: #333333;
            color: #ffffff;
            padding: 20px;
            text-align: center;
            font-size: 12px;
            line-height: 1.5;
        }
        
        .footer a {
            color: #ffffff;
            text-decoration: underline;
        }
        
        /* Responsive design for mobile */
        @media only screen and (max-width: 600px) {
            .feature-box {
                width: 100%;
                margin: 10px 0;
            }
            
            .content-wrapper {
                padding: 15px;
            }
            
            .main-title {
                font-size: 20px;
            }
        }
    </style>
</head>
